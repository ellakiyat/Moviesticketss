<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.realimage.administrator.moviesticket">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launchert"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.realimage.administrator.moviesticket.SplashScreen"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.realimage.administrator.moviesticket.activity.MainActivity"></activity>
        <activity android:name="com.realimage.administrator.moviesticket.activity.ProfileActivity"></activity>
        <activity android:name="com.realimage.administrator.moviesticket.activity.WalletActivity"></activity>
        <activity android:name="com.realimage.administrator.moviesticket.activity.ContactActivity"></activity>
        <activity android:name="com.realimage.administrator.moviesticket.activity.PpolicyActivity"></activity>
        <activity android:name="com.realimage.administrator.moviesticket.activity.PripolicyActivity"></activity>
        <activity android:name="com.realimage.administrator.moviesticket.activity.AboutActivity"></activity>
        <activity android:name="com.realimage.administrator.moviesticket.activity.TermActivity"></activity>
        <activity android:name="com.realimage.administrator.moviesticket.activity.StarActivity"></activity>
        <activity android:name=".activity.InfoActivity"></activity>
        <activity android:name=".vertical.VerticalLinearRecyclerViewSampleActivity"></activity>
    </application>

</manifest>